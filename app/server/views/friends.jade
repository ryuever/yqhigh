extends user-layout
- user = typeof(udata) != 'undefined' ? udata : { }

block script
    script(src='/vendor/jquery.form.js')
    script(src='/js/form-validators/accountValidator.js')    
    script(src='/js/views/party-dashboard.js')


block content
    block navi
        .header
            include user-navi
    #main
        block container
            #friends
                ul
                    each item in records
                        li
                            a(href="/profile/#{item.name}") #{item.name}
                        li= item.country
block footer
    #footer
        include user-footer