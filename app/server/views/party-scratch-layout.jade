extends user-layout
- user = typeof(udata) != 'undefined' ? udata : { }

block content
    .home-content
        block navi
            div.navbar
                include user-navi
        block container
            #user-content
                #temp-div
                    #party-dashboard
                        include party-dashboard
                    #party-scratch        
                        block party-scratch
block footer
    #footer
        include user-footer
block scripts
    script(src='/vendor/jquery.min.js')
    script(src='/vendor/jquery.form.js')
    script(src='/vendor/bootstrap-modal.js')
    script(src='/vendor/bootstrap-transition.js')
    script(src='/js/controllers/homeController.js')    
    script(src='/js/form-validators/accountValidator.js')
    script(src='/js/views/party-dashboard.js')
    // script(src='/js/views/home.js')
    script(src='/js/views/party-scratch.js')