extends user-layout
- user = typeof(udata) != 'undefined' ? udata : { }

block script
    script(src='/vendor/jquery.form.js')
    script(src='/js/form-validators/accountValidator.js')    
    script(src='/js/views/party-dashboard.js')
    block specified-script

block content
    block navi
        .header
            include user-navi
    #main
        block container
            .flex-nav
                include party-dashboard
            .flex-content
                #flex-partyList
                    block party-scratch
                    block pagination
            .flex-buddy
                include buddy              
block footer
    .footer
        include user-footer        
